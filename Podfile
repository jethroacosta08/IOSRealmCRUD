# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'iosdatabase' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for iosdatabase
  pod 'Realm'

  target 'iosdatabaseTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Realm/Headers'
  end

  target 'iosdatabaseUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Realm/Headers'
  end

end
